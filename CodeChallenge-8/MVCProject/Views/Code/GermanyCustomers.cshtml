@model IEnumerable<MVCProject.Models.Customer>

<h2>Customers from Germany</h2>

<table border="1">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>City</th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>@item.CustomerID</td>
        <td>@item.CompanyName</td>
        <td>@item.ContactName</td>
        <td>@item.City</td>
    </tr>
}
</table>
